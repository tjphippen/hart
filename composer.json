{
    "name": "tjphippen/hart",
    "description": "Hart Software Credit Wrapper",
    "keywords": ["hart", "credit", "score", "credit reports"],
    "homepage": "https://github.com/tjphippen/hart",
    "license": "MIT",
    "authors": [
        {
            "name": "TJ Phippen",
            "email": "tj@tjphippen.com"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "illuminate/support": "~5.1",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Tjphippen\\Hart\\": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/tjphippen/hart"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
